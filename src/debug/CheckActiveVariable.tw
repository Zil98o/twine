:: CheckVariables
<<set _checkVariables = function() {
  var variables = State.variables;
  for (var variable in variables) {
    var value = variables[variable];
    console.log(variable + ": " + value);
  }
}>>

<<link "Check Variables">><<run _checkVariables()>><</link>>
